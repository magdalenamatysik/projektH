                    background-color: #37c0bb;
                }


                #tekst{
                    width: 185px;
                    height: 100px;
                    text-align: center;
                    float: left;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 25px;
                    color: white;
                    padding-top: 50px;
                }


                #tekstt{
                    width: 380px;
                    height: 100px;
                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 30px;
                    color: white;
                    padding-top: 35px;

                }


                #add_meet{

                    width: 300px;
                    height: 220px;
                    margin: auto;
                    margin-top: 20px;
                    padding-top: 10px;
                    background-color: #a6dd5d;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;

                }


                #add_job{

                    width: 300px;
                    height: 220px;
                    margin: auto;
                    margin-top: 20px;
                    padding-top: 10px;
                    background-color: #F7EC66;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;

                }



                #add_meetbox{

                    width: 185px;
                    height: 135px;
                    margin-left:15px;
                    background-color: #a6dd5d;
                    float: left;

                }

                #add_meetbox:hover
                {
                    background-color: #a6dd5d;
                }




                #job{
                    width: 300px;
                    height: 250px;
                    margin: auto;
                    margin-top: 20px;
                    padding-top: 10px;
                    background-color: #F7EC66;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;
                }



                #list{

                    float: left;
                    width: 975px;
                    height: 280px;
                    margin:10px;
                    background-color: #666666;
                    overflow: auto;
                    color:black;

                }

                .listcolumn{

                    float: left;
                    width: 190px;
                    height: 280px;

                    background-color: #666666;

                    color:black;

                }

                #harm
                {
                    clear: both;
                    width: 1380px;
                    height: 485px;
                    margin: 10px;



                }


                .day{
                    float: left;
                    width: 185px;
                    height: 480px;
                    margin: 5px;
                    background-color: #777777;

                }

                .task{
                    float: left;
                    width: 180px;
                    margin: 4px;

                    background-color:#F7EC66;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    text-align: inherit;
                    color: gray;
                }


                .task:hover{
                    background-color:#e6db55;
                }

                .tekst2{

                    float: left;
                    width: 185px;
                    margin: 5px;
                    background-color:#999999;
                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;
                    text-decoration: underline ;

                }

                .tekst3{
                    width: 380px;
                    height: 40px;
                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 30px;
                    color: white;
                    margin-bottom: 10px;
                    background-color:#00BBBA;
                }

                .tekst4{
                    width: 150px;
                    height: 40px;
                    text-align: right;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;
                    margin-top: 72px;
                    float: left;

                }
                .tekst5{

                    height: 30px;
                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 30px;
                    color: #ffffff;
                    margin-bottom: 30px;

                }

                #logframe{
                    width: 380px;
                    height: 250px;

                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 30px;
                    color: white;
                    background-color: #00CCCB;
                    margin-left: auto;
                    margin-right: auto;
                    margin-top: 200px;


                }


                .polel{

                    width: 190px;
                    height: 20px;
                    text-align: center;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 20px;
                    color: white;
                    margin-top:10px;
                    float: left;


                }

                .poler{

                    width: 190px;
                    height: 20px;
                    text-align: left;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 15px;
                    color: white;
                    margin-top:10px;
                    float: left;


                }



                .button{
                    width: 160px;
                    height: 30px;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 18px;
                    text-align: center;
                    color: white;
                    float: right;
                    background-color:#999999;
                    box-shadow: 0 3px 3px #888888;
                    text-decoration:none;
                    margin-top: 10px;
                    margin-right: 27px;
                    margin-bottom: 10px;

                }

                a.button:hover {
                    background: #888888;

                }
                #myBtn{
                    width: 100px;
                    height: 30px;
                    margin-left:auto;
                    margin-right:auto;
                    font-family: 'Josefin Sans', sans-serif;
                    font-size: 18px;
                    color: white;
                    background-color:#00BBBA;
                    box-shadow: 0 3px 3px #00AAA5;
                    text-decoration:none;

                }

                #myBtn:hover {
                    background: #00AAA5;

                }

                .modal {
                    display: none; /* Hidden by default */
                    position: fixed; /* Stay in place */
                    z-index: 1; /* Sit on top */
                    left: 0;
                    top: 0;
                    width: 100%; /* Full width */
                    height: 100%; /* Full height */
                    overflow: auto; /* Enable scroll if needed */
                    background-color: rgb(0,0,0); /* Fallback color */
                    background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
                }

                /* Modal Content/Box */
                .modal-content {
                    background-color: gray;
                    color:#00AAA5;
                    margin: 15% auto; /* 15% from the top and centered */
                    padding: 20px;
                    border: 1px solid #888;
                    width: 380px;
                    height:250px;/* Could be more or less, depending on screen size */
                }

                /* The Close Button */
                .close {
                    color: #aaa;
                    float: right;
                    font-size: 28px;
                    font-weight: bold;
                }

                .close:hover,
                .close:focus {
                    color: black;
                    text-decoration: none;
                    cursor: pointer;
                }

            </style>






        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <meta charset="utf-8" />
        <title>Harmonogram</title>
